{
	"name": "test",
	"colors": {
		"editor.background": "#1E1E1E",
		"editor.foreground": "#eeffff",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition."
			],
			"settings": {
				"foreground": "#525252"
			}	
		},
		// const
		{
			"scope": ["variable.other.constant"],
			"settings": {
				"foreground": "#3b8cb8"
			}
		},
		// strings
		{
			"scope": ["string", "punctuation.definition.string.begin", "punctuation.definition.string.end",],
			"settings": {
				"foreground": "#9AA83A"
			}
		},
		// keywords
		{
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#9872A2"
			}
		},
		// function arguments/parameters
		{
			"scope": ["variable.parameter"],
			"settings": {
				"foreground": "#078448be",
				"fontStyle": "italic"
			}
		},
		// function arguments/parameters
		{
			"scope": ["variable.parameter"],
			"settings": {
				"foreground": "#078448be",
				"fontStyle": "italic"
			}
		},
		// numbers
		{
			"scope": [
				"constant.numeric",
			],
			"settings": {
				"foreground": "#408080"
			}
		},
		// something
		{
			"scope": [
				"keyword.operator.arithmetic",
			],
			"settings": {
				"foreground": "#F92672"
			}
		},
		// let variables, imports
		{
			"scope": [
				"variable",
			],
			"settings": {
				"foreground": "#9CDCFE"
			}
		},
		// function
		{
			"scope": [
				"meta.function",
				"entity.name.function",
				"entity.name.method",
			],
			"settings": {
				"foreground": "#d97706"
			}
		},
		// other
		{
			"scope": [
				"punctuation",
			],
			"settings": {
				"foreground": "#676867"
			}
		},
		// typescript
		{
			"scope": [
				"meta.type.annotation",
				"support.type.primitive",
			],
			"settings": {
				"foreground": "#4EC9B0"
			}
		},
		// this keyword, spread operator
			{
			"scope": [
				"variable.language.this",
				"keyword.operator.spread",
				"keyword.operator.rest"
			],
			"settings": {
				"foreground": "#C7444A"
			}
		},
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"interface": "#53b20b",
		"type": "#53b20b",
		"typeParameter": {
			"fontStyle": "italic",
			"foreground": "#078448be"
		},
	}
}